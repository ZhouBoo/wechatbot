{
    "python.jediEnabled": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/*.pyc": true,
        "**/.DS_Store": true,
        "**/*.pkl": true,
        "**/*.png": true,
        "**/beauty": true,
    },
    "python.pythonPath": "/usr/bin/python",
    "python.linting.pylintEnabled": false,
    "python.formatting.provider": "autopep8",
    "editor.formatOnSave": true,
}